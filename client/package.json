{
  "name": "binsen",
  "version": "1.0.0",
  "description": "document management system",
  "author": "Takuya Sato",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext ts,.vue --ignore-pattern *.d.ts --ignore-path .gitignore .",
    "lint-fix": "eslint --fix --ext .ts,.vue --ignore-pattern *.d.ts --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "jest"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "**/?(*.)+(spec|test).ts?(x)"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.1",
    "@nuxtjs/pwa": "^2.0.8",
    "axios": "^0.18.0",
    "marked": "^0.4.0",
    "nuxt": "^1.4.2",
    "nuxt-class-component": "^1.2.1",
    "nuxt-property-decorator": "^1.2.0",
    "uuid": "^3.3.2",
    "vee-validate": "^2.1.0-beta.8",
    "vue-i18n": "^8.0.0",
    "vuetify": "^1.1.13",
    "vuex-class": "^0.3.1"
  },
  "devDependencies": {
    "@kazupon/vue-i18n-loader": "^0.3.0",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.7.1",
    "@types/power-assert": "^1.5.0",
    "@types/vue-i18n": "^7.0.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-imports": "^1.4.1",
    "eslint": "^4.15.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-typescript": "^1.1.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-typescript": "^0.12.0",
    "eslint-plugin-vue": "^4.7.1",
    "fork-ts-checker-webpack-plugin": "^0.4.9",
    "jest": "^23.5.0",
    "lint-staged": "^7.2.2",
    "node-sass": "^4.9.3",
    "power-assert": "^1.6.0",
    "sass-loader": "^7.1.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "ts-jest": "^23.1.4",
    "ts-loader": "^3.5.0",
    "typescript": "^3.0.1",
    "typescript-eslint-parser": "^18.0.0"
  }
}
